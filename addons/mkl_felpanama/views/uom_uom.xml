<odoo>
    <data>

        <record id="felpanama_uom_form" model="ir.ui.view">
            <field name="name">felpanama_uom_form</field>
            <field name="model">uom.uom</field>
            <field name="inherit_id" ref="uom.product_uom_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='rounding']" position="after">
                    <field name="documentacion_fel"/>
                </xpath>                
            </field>
        </record>

        <record id="felpanama_uom_tree" model="ir.ui.view">
            <field name="name">felpanama_uom_tree</field>
            <field name="model">uom.uom</field>
            <field name="inherit_id" ref="uom.product_uom_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="documentacion_fel"/>
                </xpath>                
            </field>
        </record>


    </data>
</odoo>
    